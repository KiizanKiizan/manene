{
  "name": "manene",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:proxy": "next dev & local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "build": "next build",
    "start": "next build && next start -p 3001",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "npx playwright test --ui",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/lab": "^5.0.0-alpha.139",
    "@mui/material": "^5.14.0",
    "@sentry/nextjs": "^7.59.3",
    "@tanstack/react-query": "^4.29.19",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@zxing/browser": "^0.1.4",
    "@zxing/library": "^0.20.0",
    "axios": "^1.4.0",
    "axios-case-converter": "^1.1.0",
    "eslint": "^8.46.0",
    "eslint-config-next": "13.4.8",
    "eslint-plugin-react": "^7.33.1",
    "html5-qrcode": "^2.3.8",
    "next": "^13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "local-ssl-proxy": "^2.0.5",
    "msw": "^1.2.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}